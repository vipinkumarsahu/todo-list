<div class="container-fluid container-fixed-lg bg-white">
    <!-- START PANEL -->
    <div class="panel panel-transparent">
        <div class="panel-heading">
            <!-- <div class="panel-title ">Pages Default Tables Style
      </div> -->
        <div class="btn-group">
            <ul class="search-box">
                <li>
                    <div class="input-group">
                        <input type="text" id="search-table" class="form-control">
                        <span class="input-group-addon primary">
                            
                            <i class="fa fa-search"></i>
                        </span>
                    </div>
                </li>
            </ul>
            <ul class="btn-box">
                <li>
                    <div class="input-group">
                        <a href="/list-users" class="btn btn-primary">All Users</a>
                    </div>
                </li>
                <li>
                    <div class="input-group">
                        <a href="/kyc/1" class="btn btn-primary">KYC Approved</a>
                    </div>
                </li>
                <li>
                    <div class="input-group">
                        <a href="/kyc/0" class="btn btn-primary">KYC Pending</a>
                    </div>
                </li>
                <li>
                    <div class="input-group">
                        <a href="/add-user" class="btn btn-primary">Add Users</a>
                    </div>
                </li>
            </ul>
        </div>
            
        <div class="clearfix"></div>
        </div>
        <div class="panel-body">
            <table class="table table-striped tableWithSearch">
                <thead>
                    <tr>
                        <th>NAME</th>
                        <th>EMAIL</th>
                        <th>PHONE NO.</th>
                        <th>ADDRESS</th>
                        <th>VERIFICATION<br></th> 
                        <th style="width: 360px;">ACTION TO BE DONE</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (var i = 0; i < users.length; i++) { %>
                        <tr class="gradeC">
                            <td>
                                <%= users[i].name %>
                            </td>
                            <td>
                                <%= users[i].email %>
                            </td>
                            <td>
                                <%= users[i].phoneNumber %>
                            </td>
                            <td>
                                <%= users[i].address %>
                            </td>
                            <td class="social-m">
                                Email:<% if(users[i].active){ %><a><i class="fa fa-check" aria-hidden="true"></i> </a><% } else{ %> <a> <i class="fa fa-ban" aria-hidden="true"></i> </a> <% } %> <br>
                                Mobile: <% if(users[i].isNumberActive){%><a><i class="fa fa-check" aria-hidden="true"></i></a> <% } else{ %><a><i class="fa fa-ban" aria-hidden="true"></i></a> <% } %>  <br>
                                KYC: <% if(users[i].kyc.kycApprove == '1' ){ %><a><i class="fa fa-check" aria-hidden="true"></i> </a> <% } else if(users[i].kyc.kycApprove == '0'){ %> <a> <i class="fa fa-ban" aria-hidden="true"></i> </a> <% } else{  %><a> <i class="fa fa-trash" aria-hidden="true"></i> <% } %> <br>
                            </td>

                            <td class="center">
                                <div class="btn-group">
                                    <a href="/view-user/<%= users[i].id %>" class="btn btn-success" title="View">
                                        <i class="fa fa-expand"></i>
                                    </a>
                                    <a href="/edit-user/<%= users[i].id %>" class="btn btn-success" title="Edit">
                                        <i class="fa fa-edit"></i>
                                    </a>
                                    <a href="/view-kyc/<%= users[i].id %>" class="btn btn-success" title="View KYC">
                                        <i class="fa fa-expand"></i>
                                    </a>
                                    <% if(users[i].status==1){ %>
                                        <a href="javascript:;" class="btn btn-danger" title="Deactivate" data-inline-loader="1" data-post-type="1" data-parent-elm="tr"
                                            data-url="/deactivate" data-real-target="#deactive-<%= users[i].id%>" data-act="delete-modal"
                                            data-method="POST" data-post-user_id="<%=users[i].id%>" data-post-status="1" data-reload-on-success="true">
                                            <i class="fa fa-ban" aria-hidden="true"></i>
                                        </a>
                                        <% } else{ %>
                                            <a href="javascript:;" title="Activate" data-parent-elm="tr" data-real-target="#deactive-<%= users[i].id%>" data-url="/deactivate"
                                                data-post-type="1" data-post-status="2" data-inline-loader="1" data-method="POST"
                                                data-post-user_id="<%=users[i].id%>" class="btn btn-danger" data-act="delete-modal"
                                                data-reload-on-success="true">
                                                <i class="fa fa-check" aria-hidden="true"></i>
                                            </a>
                                            <% }%>
                                                <a href="javascript:;" data-act="delete-modal" data-parent-elm="tr" data-remove-parent="true" data-url="/delete-user"
                                                    data-method="POST" data-post-id="<%=users[i].id%>" class="btn btn-danger"
                                                    title="Delete">
                                                    <i class="fa fa-trash" aria-hidden="true"></i>
                                                </a>
                                </div>
                            </td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
    </div>
    <!-- END PANEL -->
</div>
<!-- end table -->